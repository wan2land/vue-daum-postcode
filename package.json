    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "push-dir": "^0.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
